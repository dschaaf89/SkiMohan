datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Season{
  id        String @id @default(uuid())
  name      String
  userId    String
  createAt  DateTime @default(now())
  updatedAt DateTime @updatedAt

}